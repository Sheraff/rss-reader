		"@tanstack/react-router": "1.147.0",
		"@tanstack/react-router-devtools": "1.147.0",
		"@tanstack/react-router-ssr-query": "1.147.0",
		"@tanstack/react-start": "1.147.0",
		"@tanstack/router-plugin": "1.147.0",
		"better-sqlite3": "12.6.0",
		"inngest": "3.49.1",
		"linkedom": "0.18.12",
		"nitro": "latest",
		"react": "19.2.3",
